    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Thomas Payer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "kiwi-suite/contract": "dev-develop",
        "kiwi-suite/entity": "dev-develop",
        "kiwi-suite/database": "dev-develop",
